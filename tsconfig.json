{
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ES2015",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "jsx": "react",
    "esModuleInterop": true,
    "types": ["node", "googlemaps"],
    "moduleResolution": "node",
    "plugins": [{"name": "typescript-plugin-css-modules"}]
  },
  "exclude": ["node_modules"]
}
